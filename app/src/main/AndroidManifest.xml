<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.worldventures.dreamtrips">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.worldventures.dreamtrips.DTApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.worldventures.dreamtrips.view.activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.worldventures.dreamtrips.view.activity.MainActivity"
            android:label="@string/title_activity_main"></activity>
    </application>

</manifest>
