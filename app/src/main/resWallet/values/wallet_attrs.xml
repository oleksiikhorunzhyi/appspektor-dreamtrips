<?xml version="1.0" encoding="utf-8"?>
<resources>
   <declare-styleable name="WalletCheckWidget">
      <attr name="icon" format="reference" />
      <attr name="title" format="string" />
      <attr name="checked" format="boolean" />
   </declare-styleable>

   <declare-styleable name="PinEntryEditText">
      <attr name="pinAnimationType" format="enum">
         <enum name="popIn" value="0" />
         <enum name="fromBottom" value="1" />
         <enum name="none" value="-1" />
      </attr>
      <attr name="pinCharacterMask" format="string" />
      <attr name="pinRepeatedHint" format="string" />
      <attr name="pinLineStroke" format="dimension" />
      <attr name="pinLineStrokeSelected" format="dimension" />
      <attr name="pinCharacterSpacing" format="dimension" />
      <attr name="pinTextBottomPadding" format="dimension" />
      <attr name="pinLineColors" format="color" />
      <attr name="pinBackgroundDrawable" format="reference" />
      <attr name="pinBackgroundIsSquare" format="boolean" />
   </declare-styleable>

   <declare-styleable name="BankCardWidget">
      <attr name="cardDrawable" format="enum">
         <enum name="blue" value="0" />
         <enum name="grey" value="1" />
         <enum name="darkblue" value="2" />
      </attr>
   </declare-styleable>

   <declare-styleable name="DirectionInfoView">
      <attr name="infoViewTitle" format="integer|string" />
      <attr name="infoViewImage" format="reference" />
   </declare-styleable>

   <declare-styleable name="CounterView">
      <attr name="counter_min_value" format="integer" />
      <attr name="counter_max_value" format="integer" />
   </declare-styleable>
</resources>